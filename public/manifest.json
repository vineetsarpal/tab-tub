{
    "manifest_version": 3,
    "name": "TabTub",
    "description": "A Chrome extension to bookmark tabs easily",
    "version": "1.0.0",
    "icons": {
        "16": "icons/icon_16x16.png",
        "32": "icons/icon_32x32.png",
        "48": "icons/icon_48x48.png",
        "128": "icons/icon.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "icons/icon.png"
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}
